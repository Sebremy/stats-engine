<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
  <children>
    <MenuBar prefHeight="21.0" prefWidth="640.0" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" onAction="#loadStatsManager" text="Stats manager" />
        <Menu mnemonicParsing="false" onAction="#loadPluginManager" text="Plugin manager" />
      </menus>
    </MenuBar>
    <AnchorPane fx:id="pluginManagerAnchorPain" maxHeight="-1.0" maxWidth="-1.0" prefWidth="641.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="257.0" text="Plugin manager">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="31.0" text="Plugin path" />
            <Button layoutX="163.0" layoutY="48.0" mnemonicParsing="false" onAction="#openBrowser" text="Browse..." />
            <TextField layoutX="14.0" layoutY="48.0" />
            <ListView layoutX="14.0" layoutY="73.0" prefHeight="263.0" prefWidth="200.0" />
            <Button layoutX="14.0" layoutY="1.0" mnemonicParsing="false" onAction="#loadStatsManager" text="Stats manager" />
            <Button layoutX="14.0" layoutY="336.0" mnemonicParsing="false" text="Add" />
            <Button layoutX="154.0" layoutY="336.0" mnemonicParsing="false" text="Remove" />
         </children>
    </AnchorPane>
  </children>
</VBox>
